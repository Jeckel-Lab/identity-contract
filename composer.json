{
    "name": "jeckel/identity-contract",
    "description": "Contract and abstract classes to manage identities in Domains",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "jeckel-lab/contract": "^1.2",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "vimeo/psalm": "^4.3",
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.8",
        "phpro/grumphp": "^0.19 || ^1.2.0"
    },
    "suggest": {
        "vimeo/psalm": "Using psalm with this 'contract' will enable immutability and strong typing verification"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Mercier-Rojas",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JeckelLab\\IdentityContract\\": "src/"
        }
    }
}
